{
  "dependencies": {
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "concurrently": "^5.3.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.9.28",
    "node-cron": "^2.0.3",
    "nodemon": "^2.0.4",
    "nunjucks": "^3.2.2",
    "ts-node": "^8.9.1",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.2"
  },
  "name": "appbox-engine",
  "version": "0.0.1",
  "description": "Engine for AppBox",
  "main": "engine.ts",
  "author": "Vic van Cooten",
  "license": "MIT",
  "scripts": {
    "startLocal": "set \"dbUrl=localhost:27017\" && yarn start",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/engine.js",
    "start": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\"  \"yarn run watch-ts\" \"yarn run watch-node\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "devDependencies": {}
}
